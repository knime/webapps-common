{
  "name": "webapps-common-demo",
  "version": "1.0.0",
  "description": "Showcase for common UI components",
  "license": "UNLICENSED",
  "author": "KNIME AG, Zurich, Switzerland",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview --port 4173",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "lint": "run-p 'lint:* -- --fix --cache'",
    "lint:js": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts",
    "lint:css": "stylelint '**/*.{css,vue}'",
    "format": "prettier --write --cache .",
    "format:check": "prettier --check --cache .",
    "deploy": "gh-pages -d dist -m 'Deploy to GitHub pages' --dotfiles",
    "preinstall": "npm i --prefix ../ && npm i --prefix ../lint"
  },
  "dependencies": {
    "prismjs": "1.29.0",
    "vue-router": "4.1.6",
    "webapps-common": "file:.."
  },
  "devDependencies": {
    "@knime/eslint-config": "file:../lint",
    "@tsconfig/node18": "^18.2.2",
    "@types/node": "18.14.2",
    "@vitejs/plugin-vue": "5.0.3",
    "@vue/tsconfig": "0.5.1",
    "eslint": "8.56.0",
    "gh-pages": "5.0.0",
    "npm-run-all": "4.1.5",
    "prettier": "3.1.1",
    "stylelint": "16.1.0",
    "typescript": "5.3.3",
    "vite": "5.0.12",
    "vite-svg-loader": "5.1.0",
    "vue-tsc": "1.8.27"
  },
  "engines": {
    "node": "18.x"
  },
  "volta": {
    "node": "18.14.2"
  }
}
