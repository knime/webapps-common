{
  "name": "webapps-common-demo",
  "version": "1.0.0",
  "description": "Showcase for common UI components",
  "license": "UNLICENSED",
  "author": "KNIME AG, Zurich, Switzerland",
  "scripts": {
    "preinstall": "npm i --prefix ../ && npm i --prefix ../lint",
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview --port 4173",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "lint": "run-p lint:*",
    "lint:js": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "lint:css": "stylelint '**/*.{css,vue}' --fix",
    "deploy": "gh-pages -d dist -m 'Deploy to GitHub pages' --dotfiles"
  },
  "dependencies": {
    "gh-pages": "3.2.3",
    "prismjs": "^1.29.0",
    "vue": "^3.2.41",
    "webapps-common": "file:.."
  },
  "devDependencies": {
    "@knime/eslint-config": "file:../lint",
    "@types/node": "^18.7.23",
    "@vitejs/plugin-vue": "^3.0.1",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "8.23.0",
    "npm-run-all": "^4.1.5",
    "postcss-preset-env": "^7.8.1",
    "stylelint": "^14.11.0",
    "typescript": "~4.7.4",
    "vite": "^3.0.4",
    "vite-svg-loader": "3.5.1",
    "vue-tsc": "^0.39.5"
  },
  "engines": {
    "node": "18.x"
  },
  "volta": {
    "node": "18.7.0"
  }
}
