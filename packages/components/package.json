    "@vue/test-utils": "2.4.4",
    "vite": "^5.2.12",
    "vite-plugin-css-injected-by-js": "^3.5.1",
    "vite-plugin-dts": "^3.9.1",
    "vite-svg-loader": "4.0.0",
    "vue-tsc": "^2.0.19"
  }
