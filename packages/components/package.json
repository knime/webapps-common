    "src",
    "!src/**/__tests__/*",
    "CHANGELOG.md"
  ],
  "module": "dist/index.js",
  "main": "./src/index.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./src/index.ts"
