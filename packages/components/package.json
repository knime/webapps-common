  },
  "dependencies": {
    "@floating-ui/vue": "1.0.2",
    "@knime/styles": "workspace:*",
    "@knime/utils": "workspace:*",
    "@vueuse/shared": "^10.10.0",
    "@vueuse/components": "^10.7.2",
    "@vueuse/core": "10.4.1",
    "date-fns": "2.30.0",
    "date-fns-tz": "2.0.0",
    "filesize": "10.0.6",
    "focus-trap-vue": "4.0.2",
    "gsap": "^3.12.5",
    "lodash-es": "4.17.21",
    "consola": "3.2.3",
    "typescript": "^5.4.5",
    "v-calendar": "3.0.3"
  },
  "peerDependencies": {
    "vue": "3.x"
  },
  "devDependencies": {
    "@knime/eslint-config": "workspace:*",
