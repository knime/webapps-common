{
  "name": "@knime/licenses",
  "version": "1.3.4",
  "description": "License checker and open source attribution collector",
  "repository": {
    "type": "git",
    "url": "https://github.com/knime/webapps-common.git",
    "directory": "packages/licenses"
  },
  "homepage": "https://knime.github.io/webapps-common/",
  "license": "GPL 3 and Additional Permissions according to Sec. 7 (SEE the file LICENSE)",
  "author": "KNIME AG, Zurich, Switzerland",
  "type": "module",
  "scripts": {
    "opensourcecredits": "node src/collect-packages"
  },
  "dependencies": {
    "commander": "12.1.0",
    "empathic": "2.0.0",
    "license-checker": "25.0.1",
    "read-installed-packages": "2.0.1",
    "semver": "6.3.1"
  },
  "peerDependencies": {
    "consola": "catalog:peer"
  },
  "main": "src/collect-packages.js",
  "files": [
    "src",
    "config",
    "CHANGELOG.md"
  ],
  "exports": {
    ".": {
      "import": "./src/collect-packages.js"
    }
  },
  "bin": {
    "license-check": "src/collect-packages.js"
  },
  "engines": {
    "node": ">=22 <25"
  },
  "packageManager": "pnpm@10.22.0"
}
