{
  "name": "webapps-common",
  "version": "0.0.0",
  "description": "Shared files for web projects. See README for details.",
  "license": "GPL 3 and Additional Permissions according to Sec. 7(SEE LICENSE IN license.txt)",
  "author": "KNIME AG, Zurich, Switzerland",
  "scripts": {
    "preinstall": "node ./install-subDependencies",
    "generate-css": "node ./ui/colors/generateCSS.js",
    "postinstall": "npm run generate-css",
    "demo": "npm --prefix demo/ install && npm run --prefix demo/ dev",
    "audit": "check-audit --production --json > target/audit.json || true",
    "lint": "npm run lint:js && npm run lint:css && npm run lint:json",
    "lint:js": "eslint --ext .js,.vue .",
    "lint:json": "eslint --ext .json .",
    "lint:css": "stylelint '**/*.{css,vue}'",
    "lint:ci": "eslint --ext .js,.vue,.json -f json -o target/eslint.json || true; stylelint '**/*.{css,vue}' -f json -o target/stylelint.json || true",
    "test:unit": "jest",
    "coverage": "jest --coverage",
    "opensourcecredits": "npm run --prefix buildtools/ opensourcecredits"
  },
  "dependencies": {
    "date-fns": "2.16.1",
    "date-fns-tz": "1.0.12",
    "filesize": "6.1.0"
  },
  "devDependencies": {
    "@babel/preset-env": "7.9.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "consola": "2.10.1",
    "cross-var": "1.1.0",
    "eslint": "7.32.0",
    "eslint-plugin-jest-formatting": "3.0.0",
    "eslint-plugin-jsonc": "2.0.0",
    "eslint-plugin-vue": "7.10.0",
    "jest": "24.9.0",
    "jest-junit": "6.4.0",
    "jest-transform-stub": "2.0.0",
    "npm-audit-resolver": "2.3.1",
    "sonar-scanner": "3.1.0",
    "stylelint": "13.6.1",
    "stylelint-config-standard": "20.0.0",
    "vue": "2.6.11",
    "vue-jest": "3.0.4",
    "vue-template-compiler": "2.6.11"
  },
  "peerDependencies": {
    "@popperjs/core": "2.5.4",
    "babel-eslint": "10.x",
    "consola": "*",
    "core-js": "3.6.5",
    "eslint": "5.x - 7.x",
    "focus-trap": "6.x",
    "focus-trap-vue": "0.x",
    "modern-normalize": "*",
    "postcss-import": "*",
    "postcss-preset-env": "6.x",
    "postcss-url": "*",
    "vue-clickaway2": "2.3.2",
    "stylelint": "10.x - 13.x",
    "stylelint-config-standard": "18.x - 20.x",
    "fontsource-roboto": "3.x",
    "fontsource-roboto-mono": "3.x",
    "v-calendar": "2.0.1"
  },
  "private": true,
  "volta": {
    "node": "14.19.0"
  }
}
