{
    "name": "knime-ui-extension-service",
    "version": "1.0.0",
    "description": "KNIME UI Extension Service",
    "license": "GPL 3 and Additional Permissions according to Sec. 7 (SEE LICENSE IN license.txt)",
    "author": "KNIME AG, Zurich, Switzerland",
    "main": "dist/index.js",
    "scripts": {
        "build": "rm -rf dist && rollup -c ./rollup.config.js",
        "dev": "webpack-dev-server --mode=development --config webpack.config.js",
        "lint": "npm run lint:ts",
        "lint:fix": "eslint --fix --ext .ts .",
        "lint:ts": "eslint --ext .ts .",
        "test": "npm run test:unit",
        "test:unit": "jest",
        "coverage": "jest --coverage",
        "sendcoverage": "cross-var sonar-scanner -Dsonar.login=\"$SONAR_LOGIN\" -Dsonar.password=\"$SONAR_PASSWORD\""
    },
    "devDependencies": {
        "@babel/cli": "^7.15.7",
        "@babel/core": "^7.15.5",
        "@babel/polyfill": "^7.12.1",
        "@babel/preset-env": "^7.15.6",
        "@babel/preset-typescript": "^7.15.0",
        "@babel/register": "^7.15.3",
        "@rollup/plugin-commonjs": "^20.0.0",
        "@types/jest": "^27.0.2",
        "@typescript-eslint/eslint-plugin": "^4.31.2",
        "@typescript-eslint/parser": "^4.31.2",
        "core-js": "^3.18.1",
        "cross-env": "^7.0.3",
        "cross-var": "1.1.0",
        "eslint": "^7.32.0",
        "eslint-config-typescript": "^3.0.0",
        "eslint-plugin-jest": "^24.4.2",
        "eslint-plugin-jest-formatting": "^3.0.0",
        "eslint-plugin-jsonc": "^1.7.0",
        "html-webpack-plugin": "^5.3.2",
        "jest": "^27.2.0",
        "jest-junit": "^12.2.0",
        "rollup": "^2.14.0",
        "rollup-plugin-copy": "^3.3.0",
        "rollup-plugin-ts": "^1.4.7",
        "sonar-scanner": "3.1.0",
        "ts-jest": "^27.0.5",
        "ts-loader": "^9.2.6",
        "typescript": "^4.4.3",
        "webpack": "^5.56.0",
        "webpack-cli": "^4.8.0",
        "webpack-dev-server": "^4.2.1"
    },
    "engines": {
        "node": "16.x"
    }
}
