{
  "name": "webapps-common",
  "version": "0.0.0",
  "description": "Shared files for web projects. See README for details.",
  "homepage": "https://knime.github.io/webapps-common/",
  "license": "GPL 3 and Additional Permissions according to Sec. 7 (SEE LICENSE IN license.txt)",
  "author": "KNIME AG, Zurich, Switzerland",
  "type": "module",
  "scripts": {
    "dev": "npm --prefix demo/ install && npm run --prefix demo/ dev",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "run-p 'lint:* -- --fix'",
    "lint:js": "eslint . --cache --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts",
    "lint:css": "stylelint --cache '**/*.{css,vue}'",
    "format": "prettier --cache --write .",
    "format:check": "prettier --cache --check .",
    "ci:lint-format": "run-p ci:lint:* format:check -c",
    "ci:lint:js": "npm run lint:js && npm run lint:js -- -f json -o test-results/eslint.json",
    "ci:lint:css": "npm run lint:css -- -f json -o test-results/stylelint.json",
    "audit": "npm audit --omit dev",
    "test:unit": "TZ='Europe/Berlin' vitest",
    "coverage": "TZ='Europe/Berlin' vitest run --coverage",
    "generate-css": "node ./ui/colors/generateCSS.mjs",
    "opensourcecredits": "npm run --prefix buildtools/ opensourcecredits",
    "preinstall": "node ./install-subDependencies.cjs",
    "postinstall": "run-p generate-css opensourcecredits",
    "prepare": "husky install"
  },
  "dependencies": {
    "@floating-ui/vue": "1.0.2",
    "@fontsource/roboto": "4.5.8",
    "@fontsource/roboto-mono": "4.5.10",
    "@knime/ui-extension-service": "0.32.0",
    "@tiptap/extension-link": "2.0.3",
    "@tiptap/extension-text-align": "2.0.3",
    "@tiptap/extension-underline": "2.0.3",
    "@tiptap/pm": "2.0.3",
    "@tiptap/starter-kit": "2.0.3",
    "@tiptap/vue-3": "2.0.3",
    "@vueuse/components": "^10.7.2",
    "@vueuse/core": "10.4.1",
    "consola": "2.15.3",
    "date-fns-tz": "2.0.0",
    "filesize": "10.0.6",
    "focus-trap-vue": "4.0.2",
    "gsap": "3.12.2",
    "lodash-es": "4.17.21",
    "modern-normalize": "1.1.0",
    "v-calendar": "3.0.0-alpha.8"
  },
  "devDependencies": {
    "@knime/eslint-config": "8.0.11",
    "@tsconfig/node20": "20.1.2",
    "@types/jsdom": "20.0.1",
    "@types/lodash-es": "4.17.12",
    "@types/node": "20.11.6",
    "@vitejs/plugin-vue": "5.0.3",
    "@vitest/coverage-v8": "1.2.1",
    "@vue/test-utils": "2.4.4",
    "@vue/tsconfig": "0.5.1",
    "eslint": "8.56.0",
    "husky": "8.0.3",
    "jsdom": "20.0.3",
    "lint-staged": "15.1.0",
    "npm-run-all": "4.1.5",
    "postcss-import": "15.1.0",
    "postcss-mixins": "9.0.4",
    "postcss-preset-env": "8.0.0",
    "postcss-url": "10.1.3",
    "prettier": "3.1.1",
    "stylelint": "16.1.0",
    "typescript": "5.3.3",
    "vite-svg-loader": "5.1.0",
    "vitest": "1.2.1",
    "vue": "3.4.21",
    "vue-tsc": "1.8.27"
  },
  "engines": {
    "node": "18.x - 20.x"
  },
  "volta": {
    "node": "20.11.0"
  }
}
