{
  "name": "@knime/ui-extension-service",
  "version": "0.26.0",
  "description": "KNIME UI Extension Service for KNIME Analytics Platform",
  "license": "GPL 3 and Additional Permissions according to Sec. 7 (SEE LICENSE IN license.txt)",
  "author": "KNIME AG, Zurich, Switzerland",
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/KNIME/knime-ui-extension-service"
  },
  "scripts": {
    "build": "rm -rf dist && rollup -c ./rollup.config.js",
    "format": "prettier --cache --write .",
    "format:check": "prettier --cache --check .",
    "lint": "eslint --cache --fix --ext .ts .",
    "ci:lint": "eslint --cache --ext .ts,.vue -f json -o test-results/eslint.json",
    "ci:lint-format": "run-p ci:lint format:check -c",
    "audit": "npm audit --omit=dev",
    "test": "npm run test:unit",
    "test:unit": "jest",
    "coverage": "jest --coverage --runInBand",
    "coverage:watch": "npm run test:unit -- --coverage --watch",
    "prepare": "husky"
  },
  "dependencies": {
    "color-convert": "2.0.1",
    "uuid": "9.0.1"
  },
  "devDependencies": {
    "@babel/cli": "7.23.9",
    "@babel/core": "7.23.9",
    "@babel/polyfill": "7.12.1",
    "@babel/preset-env": "7.23.9",
    "@babel/preset-typescript": "7.23.3",
    "@babel/register": "7.23.7",
    "@knime/eslint-config": "8.0.11",
    "@rollup/plugin-commonjs": "25.0.7",
    "@types/color-convert": "2.0.3",
    "@types/jest": "29.5.12",
    "@types/uuid": "9.0.8",
    "core-js": "3.36.0",
    "eslint": "8.57.0",
    "flush-promises": "1.0.2",
    "husky": "9.0.11",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-junit": "16.0.0",
    "lint-staged": "15.2.2",
    "npm-run-all": "4.1.5",
    "prettier": "3.2.5",
    "rollup": "2.79.1",
    "rollup-plugin-terser": "7.0.2",
    "rollup-plugin-ts": "3.4.5",
    "ts-jest": "29.1.2",
    "ts-loader": "9.5.1",
    "typescript": "5.3.3"
  },
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "engines": {
    "node": "18.x - 20.x"
  },
  "volta": {
    "node": "20.11.1"
  }
}
